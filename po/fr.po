"'command -v cairo-compile >/dev/null || export "
"PATH=\"$CAIRO_ROOT/target/release:$PATH\"'"
msgstr ""
"'command -v cairo-compile >/dev/null=\"export "
"PATH=\"$CAIRO_ROOT/cible/release:$PATH\"'"

